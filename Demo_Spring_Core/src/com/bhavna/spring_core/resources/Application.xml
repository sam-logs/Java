<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="StudentObj" class="com.bhavna.spring_core.bean.Student">
		<property name="rollNo">
			<value>101</value>
		</property>
		<property name="name">
			<value>Ram</value>
		</property>
		<property name="salary">
			<value>40000</value>
		</property>
		<property name="school">
			<list>
				<value>Chandigarh University</value>
				<value>LPU</value>
				<value>Chitkara</value>
			</list>
		</property>

		<property name="standard">
			<set>
				<value>First Year</value>
				<value>Second Year</value>
				<value>Second Year</value>

			</set>
		</property>

		<property name="uid">
			<map>
				<entry key="181903" value="Ram"></entry>
				<entry key="181904" value="Jam"></entry>

			</map>
		</property>

	</bean>

</beans>